	img{width:200px; height:200px; border:5px solid $ddd;}
</style>
<script>
	$(function(){
		//body의 첫번째 이미지가 click이벤트 발생
		var imgTag = "<img src='../img/02.jfif'/>";
		$('img').first().on('click',function(){
			$('body').append(imgTag);
		})
	});
</script>
