<script>
	$(function(){
		$("#f>li:nth-child(3n)").css('color','orange');
		
		//addClass() - 클래스 추가
		//remobeClass() - 클래스 지우기
		//toggleClass()
		
		$("#f>li:first").addClass('c1');
		$("#f>li:first").removeClass('c1');
		
		$("#f>li").eq(2).addClass('c2');
	});
</script>
